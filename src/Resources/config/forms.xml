<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <!-- Forms-->
        <service id="hmlb_user.form.type.command.change_email" class="HMLB\UserBundle\Form\Type\Command\ChangeEmailType">
            <tag name="form.type" alias="change_email"/>
        </service>
        <service id="hmlb_user.form.type.command.change_password" class="HMLB\UserBundle\Form\Type\Command\ChangePasswordType">
            <tag name="form.type" alias="change_password"/>
        </service>
    </services>
</container>
